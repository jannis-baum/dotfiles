{
    "panel": {
        "keepCentered": true
    },
    "statusInfo": {
        "placeholder": {
            "font": "Poiret One",
            "elements": [
                {
                    "updateFrequency": 5,
                    "main": { "path": "~/.synapse/status/battery.zsh" },
                    "alt": { "path": "~/.synapse/status/battery.zsh", "args": ["--percentage"] }
                },
                {
                    "updateFrequency": 1,
                    "main": { "path": "/bin/date", "args": ["+%l:%M"] },
                    "alt": { "path": "/bin/date", "args": ["+%m/%d"] }
                }
            ]
        }
    },
    "search": {
        "program": { "path": "/usr/local/bin/fzf", "args": ["--delimiter=:", "--nth=2..", "-f"] }
    },
    "modules": [
        {  "/native/controller": { "trigger": "^sc (.*)" } },
        { "/native/switcher": {
            "name": "",
            "trigger": "(^(?!p )(?!u )(?!c )(?!m )(?!sc )(?!pass )(?!pkill ).*)"
        } },
        { "app-search": {
            "trigger": "(^(?!p )(?!u )(?!c )(?!m )(?!sc )(?!pass )(?!pkill ).+)",
            "maxResults": 3
        } },
        { "/native/clipboard": { "trigger": "^p (.*)" } },
        "unicode",
        "control-center",
        "cas",
        "keychain",
        "pkill"
    ]
}
